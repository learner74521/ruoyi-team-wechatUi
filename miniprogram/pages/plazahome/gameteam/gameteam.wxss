/* pages/plazahome/gameteam/gameteam.wxss */
page{
  background-color: white;
}
.nav-li {
  color: white;
  position: relative;
  overflow: hidden;
  text-shadow: 2px 2px 2px #000;
}

.nav-icon {
  float: right;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  height: 75rpx;
  width: 75rpx;
  border-radius: 8rpx;
}

.nav-back {
  background-size: cover;
  background-blend-mode: darken;
  background-repeat: no-repeat;
  background-position: center;
  transform: scale(1.2);
  position: absolute;
  top: 0px;
  bottom: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  z-index: -1;
  filter: blur(3px);
}
