<!--pages/plazahome/gameteam/gameteam.wxml-->
<cu-custom bgColor="bg-gradual-pink" isBack="{{true}}">
  <view slot="backText">返回</view>
  <view slot="content">游戏</view>
</cu-custom>
<swiper-item>
  <view class='swiper_con'>
    <!-- 内容列表 -->
    <scroll-view scroll-y class="scrollPage">
      <view class='nav-list'>
        <navigator open-type="navigate" hover-class='none' url="/pages/basics/{{item.name}}/{{item.name}}" class="nav-li " wx:for="{{elements}}" wx:key bindtap="toChildPage" data-title="{{item.title}}">
          <image class="nav-back" src="{{item.bgimg}}"></image>
          <view class="nav-icon" style="background-image:url({{item.icon}})"></view>
          <view class="nav-title">{{item.title}}</view>
          <view class="nav-name">{{item.name}}</view>
        </navigator>
      </view>
    </scroll-view>
  </view>
</swiper-item>